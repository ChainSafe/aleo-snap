{
  "name": "aleo-snap",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "yarn run build:snap",
    "build:bareminimum": "mm-snap build",
    "build:snap": "yarn build:snap:bundle && yarn build:snap:eval",
    "build:snap:bundle": "mm-snap build --eval false",
    "build:snap:eval": "mm-snap eval -b dist/bundle.js --verboseErrors",
    "lint": "eslint --color --ext .ts src/",
    "lint:fix": "yarn run lint --fix",
    "test:unit": "echo",
    "test:integrations": "echo"
  },
  "dependencies": {
    "aleo-snap-wasm": "workspace:*"
  },
  "devDependencies": {
    "@metamask/snaps-cli": "^0.32.2",
    "@metamask/snaps-types": "^0.32.2",
    "@types/eslint": "^8.37.0",
    "@types/node": "18.15.11",
    "eslint": "^8.37.0",
    "typescript": "^5.0.3"
  }
}
