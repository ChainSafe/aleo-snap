{
  "version": "1.2.1",
  "description": "The Aleo snap.",
  "proposedName": "Aleo",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chainsafe/aleo-snap.git"
  },
  "source": {
    "shasum": "FDQk/+MxtfBk/Qv8R3SGkA9EP93a1N0Ub29FEmZAEnA=",
    "location": {
      "npm": {
        "filePath": "dist/bundle.js",
