{
  "name": "aleo-snap-wasm",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "yarn stringify:wasm && yarn build:bundle && yarn copy:dts",
    "build:bundle": "tsc --build && spack",
    "copy:dts": "mkdir ./lib/wasmBuild && cp ./src/wasmBuild/aleo_wasm.d.ts ./lib/wasmBuild/aleo_wasm.d.ts",
    "stringify:wasm": "node -r ts-node/register ./scripts/stringifyWasm.ts"
  },
  "main": "lib/index.js",
  "browser": "lib/index.js",
  "types": "lib/index.d.ts",
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.57",
    "regenerator-runtime": "^0.13.11",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@aleohq/wasm": "^0.4.1"
  }
}
