{
  "$schema": "http://json.schemastore.org/swcrc",
  "jsc": {
      "parser": {
        "syntax": "typescript",
        "tsx": false,
        "decorators": false,
        "dynamicImport": false
      },
      "target": "es2015"
    },
  "module": {
      "type": "umd",
      "globals": {},

      // These are defaults.
      "strict": false,
      "strictMode": true,
      "lazy": false,
      "noInterop": false
    }
}